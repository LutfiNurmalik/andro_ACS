<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.regulasiudara.aircargoshippingguidelines">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_acslogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_acslogo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splashscreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registrasi"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Login" />
        <activity android:name=".DangerousGoodsAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".ClassificationAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".SegregationAct" />
        <activity android:name=".Artikel_Classification" />
        <activity android:name=".PackingInstructionAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".ArtikelPackingInstruction" />
        <activity android:name=".LimitationAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".LimCountry"
            android:screenOrientation="portrait"/>
        <activity android:name=".ArtikelLimCountry" />
        <activity android:name=".LimAirline"
            android:screenOrientation="portrait"/>
        <activity android:name=".ArtikelLimAirline" />
        <activity android:name=".ProperShippingNameAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".AboutAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".ContactAct"
            android:screenOrientation="portrait"/>
    </application>

</manifest>